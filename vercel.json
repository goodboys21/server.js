{
  "version": 2,
  "name": "server-jees2",
  "builds": [
    { "src": "app.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/image/(.*)",
      "dest": "/image/$1"
    },
    {
      "src": "/upload",
      "methods": ["POST"],
      "dest": "app.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1",
      "check": true
    },
    {
      "src": "/.*",
      "dest": "/public/index.html"
    }
  ]
}
