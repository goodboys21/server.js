{
  "version": 2,
  "name": "server-jees2",
  "builds": [
    { "src": "app.js", "use": "@vercel/node" },
    { "src": "image/**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/image/(.*)",
      "dest": "/image/$1"
    },
    {
      "src": "/upload",
      "dest": "app.js"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
